import java.awt.print.Book;

public class BankInfo
{
	public Bank create()
	{
		Bank bank1 = new Bank(0,"mahesh", 101101, "bhondemahesh9999", 123123,	 10000, "dighi");
		return bank1;
	}
	
	public void display(Bank bank1)
	{

		System.out.println("banck account number is:"+bank1.getAccNo());
		System.out.println("bank account holder name is :"+bank1.getAccName());
		System.out.println("bank IFSC code:"+bank1.getAccifcCode());
		System.out.println("bank account email address:"+bank1.getAccEmail());
		System.out.println(" bank account holder phone number:"+bank1.getAccPhone());;
		System.out.println("bank balance is:"+bank1.getAccBalance());
		System.out.println(" bank address id :"+bank1.getAccAddress());
	}	
}
