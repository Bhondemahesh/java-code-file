import java.util.Scanner;

public class Arrayinfo
{
	Scanner sc = new Scanner(System.in);
	int[][] arr1 = new int[20][20];	
	int[][] arr2 = new int[20][20];
	int r1,c1; 
	public int array()
		{
			System.out.println("Enter Row or Column first matrix ");
			 r1 = sc.nextInt();
			 c1 = sc.nextInt();
			for (int i = 0; i < r1; i++)
			{
				for (int j = 0; j <c1 ; j++) 
				{
					arr1[i][j] = sc.nextInt();
				}
			}
			return 0;
		}
	 public void display(Arrayinfo
			 array) 
	 {
			for (int i = 0; i < r1; i++)
			{
				for (int j = 0; j <c1 ; j++) 
				{
					System.out.println(+arr1[i][j]);
				}
			}
		}
	 }

